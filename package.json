{
  "name": "mupee",
  "version": "1.0.1",
  "author": "Linagora",
  "description":
    "Mupee is a proxy between the Mozilla.org update servers and your corporate Mozilla software.",
  "engines": {
    "node": ">=0.10"
  },
  "scripts": {
    "start": "node app.js",
    "test": "sh ./scripts/test-backend.sh && sh ./scripts/test-frontend.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linagora/mupee.git"
  },
  "dependencies": {
    "express": "3.4.x",
    "request": "2.27.x",
    "jade": "*",
    "mongoskin": "0.6.x",
    "mongodb": "1.3.x",
    "jstoxml": "0.2.x",
    "libxml-to-js": "0.3.x",
    "winston": "0.7.x",
    "nconf": "0.6.x",
    "mkdirp": "0.3.x"
  },
  "devDependencies": {
    "mocha": "*",
    "chai": "*",
    "nock": "*",
    "phantomjs" : "*",
    "karma" : "*",
    "karma-junit-reporter" : "*",
    "karma-jasmine" : "*",
    "karma-ng-scenario" : "*",
    "karma-phantomjs-launcher": "*",
    "karma-mocha": "*",
